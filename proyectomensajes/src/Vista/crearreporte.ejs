<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <title>Drivin - Driving School Website Template</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="" name="keywords">
    <meta content="" name="description">

    <!-- Favicon -->
    <link href="img/favicon.ico" rel="icon">

    <!-- Google Web Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Work+Sans:wght@400;500;600;700&display=swap" rel="stylesheet">

    <!-- Icon Font Stylesheet -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css" rel="stylesheet">

    <!-- Libraries Stylesheet -->
    <link href="lib/animate/animate.min.css" rel="stylesheet">
    <link href="lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">

    <!-- Customized Bootstrap Stylesheet -->
    <link href="css/bootstrap.min.css" rel="stylesheet">

    <!-- Template Stylesheet -->
    <link href="css/style.css" rel="stylesheet">
</head>

<body>
    <!-- Spinner Start -->
    <div id="spinner"
        class="show bg-white position-fixed translate-middle w-100 vh-100 top-50 start-50 d-flex align-items-center justify-content-center">
        <div class="spinner-grow text-primary" role="status"></div>
    </div>
    <!-- Spinner End -->


    <!-- Topbar Start -->
    <div class="container-fluid bg-dark text-light p-0">
        <div class="row gx-0 d-none d-lg-flex">
            <div class="col-lg-7 px-5 text-start">
                <div class="h-100 d-inline-flex align-items-center me-4">
                    <small class="fa fa-map-marker-alt text-primary me-2"></small>
                    <small>Av. Heroínas N°111 esq. España, Cochabamba</small>
                </div>

            </div>
            <div class="col-lg-5 px-5 text-end">
                <div class="h-100 d-inline-flex align-items-center me-4">
                    <small class="fa fa-phone-alt text-primary me-2"></small>
                    <small>4 4222741</small>
                </div>
                <div class="h-100 d-inline-flex align-items-center mx-n2">
                    <a class="btn btn-square btn-link rounded-0 border-0 border-end border-secondary"
                        href="https://www.facebook.com/Lapromotoraefv"><i class="fab fa-facebook-f"></i></a>
                    <a class="btn btn-square btn-link rounded-0 border-0 border-end border-secondary"
                        href="https://www.linkedin.com/company/la-promotora-efv/"><i class="fab fa-linkedin-in"></i></a>
                    <a class="btn btn-square btn-link rounded-0" href="https://www.instagram.com/lapromotoraefv/"><i
                            class="fab fa-instagram"></i></a>
                </div>
            </div>
        </div>
    </div>
    <!-- Topbar End -->


    <!-- Navbar Start -->
    <nav class="navbar navbar-expand-lg bg-white navbar-light sticky-top p-0">
        <a href="index.html" class="navbar-brand d-flex align-items-center border-end px-4 px-lg-5">
            <h2 class="m-0"><i></i>La promotora</h2>
        </a>
        <button type="button" class="navbar-toggler me-4" data-bs-toggle="collapse" data-bs-target="#navbarCollapse">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarCollapse">
            <div class="navbar-nav ms-auto p-4 p-lg-0">
                <ul class="nav nav-pills">
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" data-bs-toggle="dropdown" href="#" role="button"
                            aria-expanded="false">Mensaje</a>
                        <ul class="dropdown-menu">
                            <li><a class="dropdown-item" href="/crearmensaje">Crear Mensaje</a></li>
                            <li><a class="dropdown-item" href="/vermensaje">Ver Mensaje</a></li>
                            <li><a class="dropdown-item" href="/enviarmensaje">Enviar Mensaje</a></li>
                        </ul>
                    </li>
                </ul>
                <a href="/enviarmasivo" class="nav-item nav-link">Programar Envio</a></li>
                <a href="/saldo" class="nav-item nav-link">Saldo</a></li>
                <a href="/crearreporte" class="nav-item nav-link">Reporte</a>
                <a href="/cerrarsesion" class="nav-item nav-link">Cerrar Sesión</a>
            </div>
    
        </div>
    </nav>
    <!-- Navbar End -->


    <!-- Page Header Start -->
    <div class="container-fluid page-header py-6 my-6 mt-0 wow fadeIn" data-wow-delay="0.1s">
        <div class="container text-center">
            <h1 class="display-4 text-white animated slideInDown mb-4">Crear Reporte De Mensajes</h1>
        </div>
    </div>
    <!-- Page Header End -->


    <!-- Appointment Start -->
    <div class="bg-primary text-center p-2" style="margin-bottom: 100px;">
        <h1 class="mb-4">Filtra Los Datos Para El Reporte</h1>
        <!--Formulario para buscar en la base datos POST-->
        <div id="todo" class="container text-center">
            <form action="/buscarreporte" method="POST">
                <div class="form-group row">
                    <div class="col">
                        <label for="colFormLabel" style="color: black;" class="col-sm-2 col-form-label">Buscar Por
                            Usuario</label>
                        <input type="text" class="form-control" name="bususuario" placeholder="Ingresa el Usuario">
                    </div>
                    <div class="col">
                        <label for="colFormLabel" style="color: black;" class="col-sm-2 col-form-label">Buscar Por
                            Fecha</label>
                        <input type="text" class="form-control" name="busfecha" placeholder="Ingresa la Fecha">
                    </div>
                </div>
                <div class="form-group row mt-5">
                    <button class="btn btn-dark w-100 py-3">Generar Tabla</button>
                </div>
            </form>
        </div>
    </div>
    <div class="container">
        <% if(datos!="no" ){%>
            <table class="table" style="margin-bottom: 100px;">
                <thead class="table-dark">
                    <th>#</th>
                    <th>codigo</th>
                    <th>nombre</th>
                    <th>numero</th>
                    <th>plaza</th>
                    <th>agencia</th>
                    <th>mensaje</th>
                    <th>fecha envio</th>
                    <th>hora envio</th>
                    <th>enviador</th>
                    <th>chip origen</th>

                </thead>
                <tbody>
                    <%for(var i=0;i<datos.length;i++){%>
                        <tr>
                            <td>
                                <%=i+1%>
                            </td>
                            <td>
                                <%=datos[i].COD_CLI%>
                            </td>
                            <td>
                                <%=datos[i].APELLIDOS_NOMBRES%>
                            </td>
                            <td>
                                <%=datos[i].NRO_CEL%>
                            </td>
                            <td>
                                <%=datos[i].PLAZA%>
                            </td>
                            <td>
                                <%=datos[i].AGENCIA%>
                            </td>
                            <td>
                                <%=datos[i].MENSAJE%>
                            </td>
                            <td>
                                <%=datos[i].FECHA_ENVIO%>
                            </td>
                            <td>
                                <%=datos[i].HORA_ENVIO%>
                            </td>
                            <td>
                                <%=datos[i].ENVIADOR%>
                            </td>
                            <td>
                                <%=datos[i].CHIP%>
                            </td>
                        </tr>
                        <%}%>
                </tbody>
            </table>
            <%}%>

    </div>
    <div class="bg-primary text-center p-2" style="margin-bottom: 100px;">
        <form action="/descargarexcel" method="POST">
            <button class="btn btn-dark w-100 py-3">Generar Reporte</button>
        </form>
    </div>




    <!-- Appointment End -->




    <!-- Copyright Start -->
    <div class="container-fluid copyright text-center py-4 wow fadeIn" style="margin-top: 100px;" data-wow-delay="0.1s">
        &copy; <a href="#">La Promotora EFV </a>
        <p style="color:aliceblue"> 2023 Todos los Derechos Reservados.</p>
    </div>
    <!-- Copyright End -->




    <!-- JavaScript Libraries -->
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="lib/wow/wow.min.js"></script>
    <script src="lib/easing/easing.min.js"></script>
    <script src="lib/waypoints/waypoints.min.js"></script>
    <script src="lib/owlcarousel/owl.carousel.min.js"></script>

    <!-- Template Javascript -->
    <script src="js/main.js"></script>
</body>

</html>